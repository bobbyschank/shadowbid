          </div>  
         </div>
    </div>
<!-- END OF UNEXPANDED BID WINDOW -->

    <div id="collapse1" class="panel-collapse collapse">
      <div class="panel-body">
        <div class="col-sm-8">
          <img class="expanded-graph" src="/images/graph-example1.png">
        </div>
        <div class="col-sm-4">
          <h2>Edit Bid</h2>
              <div class="form-group">
                <label for="bidAmount">Bid Price:</label>
                  <div class="form-group">
                    <div class="input-group">
                      <div class="input-group-addon">$</div>
                      <input type="text" class="form-control" id="bidPrice" placeholder="Bid Price">
                    </div>
                  </div>
              </div>
          <span>Quantity:</h4><input type="text" class="form-control" id="exampleInputAmount" placeholder="Amount">
          <h4>Duration:</h4><input type="text" class="form-control" id="exampleInputAmount" placeholder="Amount">
          <button class="btn-primary">Update</button>
        </div>
      </div>
      
    </div>
  </div>
</div>
